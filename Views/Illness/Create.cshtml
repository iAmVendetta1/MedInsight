        Blackheads: document.getElementById("Blackheads").checked ? 1 : 0,
        Scurring: document.getElementById("Scurring").checked ? 1 : 0,
        SkinPeeling: document.getElementById("SkinPeeling").checked ? 1 : 0,
        SilverLikeDusting: document.getElementById("SilverLikeDusting").checked ? 1 : 0,
        SmallDentsInNails: document.getElementById("SmallDentsInNails").checked ? 1 : 0,
        InflammatoryNails: document.getElementById("InflammatoryNails").checked ? 1 : 0,
        Blister: document.getElementById("Blister").checked ? 1 : 0,
        RedSoreAroundNose: document.getElementById("RedSoreAroundNose").checked ? 1 : 0,
        YellowCrustOoze: document.getElementById("YellowCrustOoze").checked ? 1 : 0,
                    };
            try {
                const response = await fetch('@Url.Action("Predict", "Illness")', {
                    method: "POST",
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(symptoms)
                });
                const data = await response.json();
                document.getElementById("Prognosis").value = data.prediction;
            } catch (error) {
                console.error("Error predicting prognosis:", error);
            }
        }

        // Attach getPrediction to Prognosis field focus
        document.getElementById("predictButton").addEventListener("click", getPrediction);
    </script>
